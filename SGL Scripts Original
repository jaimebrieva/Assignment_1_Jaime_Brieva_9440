    LocationSK          INT64       NOT NULL, 
    Borough             STRING      NOT NULL,
    Geo_Type_Name       STRING      NOT NULL,
    Geo_Place_Name      STRING      NOT NULL,
   
    GeoID               STRING
);


CREATE TABLE IF NOT EXISTS `assignment1-9440-jaime-brieva.assignment19440.DimIndicator` (
    IndicatorSK         INT64       NOT NULL,
    Indicator_Name      STRING      NOT NULL,
    Measure             STRING,
    Measure_Info        STRING,
    Data_Value_Type     STRING
);


